{
  "name": "eleme",
  "manifest_version": 2,
  "icons": {"128": "icon.jpeg"},
  "version": "1.0",
  "description": "eleme upload pic",
  "permissions": ["tabs","clipboardRead","clipboardWrite","*://*/*"],
  "browser_action": {
    "default_title": "eleme",
    "default_icon": "icon.jpeg"
  },
  "content_scripts": [
    {
      "matches": ["http://mygoare.com/*"],
      "js": ["js/jquery.js"]    /*only mygoare.com has jquery liblary*/
    }
  ],
  "background": {
    "scripts": ["bg.js"]
  }
}
