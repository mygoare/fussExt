{
  "name": "eleme",
  "manifest_version": 2,
  "icons": {"128": "icon.jpeg"},
  "version": "1.0",
  "description": "eleme upload pic",
  "permissions": ["tabs","clipboardRead","clipboardWrite","*://u.com/*"],
  "browser_action": {
    "default_title": "eleme",
    "default_icon": "icon.jpeg"
  },
  "content_scripts": [
    {
      "matches": ["*://github.com/Eleme/*/issues/*"],
      "js": ["js/jquery.js", "js/jquery.form.js", "js/upload.js"],
      "css": ["upload.css"]
    }
  ]
}
